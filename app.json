{
  "expo": {
    "name": "Best Beast",
    "slug": "beanbeast",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#363533"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zachswinehart.beanbeast"
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "package": "com.zachswinehart.bean_beast",
      "versionCode": 4
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "websites-for-less-llc",
            "project": "bean-beast",
            "authToken": "6426d7bafe9847a9b2e667ec2f269cbf2c13e05f72d94daaba083823c38aad9f"
          }
        }
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "svg"]
    }
  }
}
